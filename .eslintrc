{
  "extends": [
    "babel",
    "eslint:recommended"
  ],
  "env": {
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module"
  },
  "rules": {
    "quotes": [2, "single"],
    "semi": [2, "always"],
    "eqeqeq": ["error", "always"],
    "no-bitwise": ["error"],
    "no-use-before-define": ["error", {"functions": false, "classes": false}],
    "comma-dangle": "error"
  }
}
